{
  "name": "@reforged/maker-appimage",
  "version": "4.0.2",
  "description": "An AppImage maker implementation for the Electron Forge.",
  "main": "./dist/cjs/main.js",
  "module": "./dist/esm/main.js",
  "types": "./types/main.d.ts",
  "scripts": {
    "prepack": "npm run prepack:esm && npm run prepack:cjs",
    "prepack:esm": "tsc --module es2022 --moduleResolution bundler --target es2022 --outDir ./dist/esm",
    "prepack:cjs": "tsc --module commonjs --moduleResolution node --target es2022 --outDir ./dist/cjs"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/SpacingBat3/ReForged"
  },
  "keywords": [
    "electron",
    "electron-forge",
    "maker",
    "reforged",
    "packaging",
    "appimage"
  ],
  "author": "SpacingBat3",
  "license": "ISC",
  "dependencies": {
    "@electron-forge/maker-base": "^6.0.0 || ^7.0.0",
    "@spacingbat3/lss": "^1.0.0",
    "semver": "^7.3.8"
  },
  "publishConfig": {
    "access": "public"
  },
  "engines": {
    "node": ">=19.0.0 || ^18.11.0"
  },
  "exports": {
    ".": {
      "import": "./dist/esm/main.js",
      "require": "./dist/cjs/main.js",
      "types": "./types/main.d.ts"
    }
  }
}
